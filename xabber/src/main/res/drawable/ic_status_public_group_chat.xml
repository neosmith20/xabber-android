<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape
            android:shape="oval">
            <solid
                android:color="?attr/contact_list_contact_background"/>
            <size
                android:height="16dp"
                android:width="16dp"/>
        </shape>

    </item>

    <item
        android:top="1dp"
        android:left="1dp"
        android:right="1dp"
        android:bottom="1dp">
        <shape
            android:shape="oval">
            <solid
                android:color="@color/light_green_500"/>
        </shape>
    </item>

    <item
        android:drawable="@drawable/ic_group_public"/>

</layer-list>